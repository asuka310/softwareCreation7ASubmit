<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="153.0" prefWidth="295.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.CheckDeleteController">
   <children>
      <Button fx:id="yes_button" layoutX="67.0" layoutY="90.0" mnemonicParsing="false" onAction="#deleteItem" text="はい" />
      <Button fx:id="no_button" layoutX="185.0" layoutY="90.0" mnemonicParsing="false" onAction="#closeWindow" text="いいえ" />
      <Label fx:id="confirmlabel" layoutX="67.0" layoutY="32.0" prefHeight="25.0" prefWidth="173.0" text="本当にこのデータを削除しますか？">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <Label fx:id="deletetitle" alignment="CENTER" layoutX="76.0" layoutY="68.0" prefHeight="17.0" prefWidth="155.0" />
      <Label fx:id="finishdelete" alignment="CENTER" layoutX="58.0" layoutY="130.0" prefHeight="17.0" prefWidth="181.0" />
   </children>
</AnchorPane>
