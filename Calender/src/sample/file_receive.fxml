<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.FileReceiveController">
   <children>
      <TableView fx:id="file_name_size_tableID" layoutX="37.0" layoutY="34.0" prefHeight="284.0" prefWidth="551.0" stylesheets="@../cssFolder/tableViewDesign01.css">
        <columns>
          <TableColumn fx:id="file_name_table2ID" editable="false" prefWidth="427.0" sortable="false" text="file name" />
          <TableColumn fx:id="file_size_table2ID" editable="false" prefWidth="120.0" sortable="false" text="size" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button layoutX="495.0" layoutY="342.0" mnemonicParsing="false" onAction="#saveSelectedFile" stylesheets="@../cssFolder/buttonDesign03.css" text="保存" />
      <Button layoutX="411.0" layoutY="342.0" mnemonicParsing="false" onAction="#closeWindow" stylesheets="@../cssFolder/buttonDesign04.css" text="閉じる" />
   </children>
</AnchorPane>
